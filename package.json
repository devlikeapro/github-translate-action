{
  "name": "custom-github-translator",
  "version": "3.0.0",
  "private": true,
  "description": "The action for translating Non-English issues content to English.",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsup",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "cross-env NODE_ENV=production eslint src --fix --ext .ts,.js",
    "test": "jest",
    "all": "npm run format && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lizheming/github-translate-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "translate",
    "issues",
    "localization"
  ],
  "author": "CaoMeiYouRen",
  "contributors": [
    "tomsun28",
    "lizheming",
    "CaoMeiYouRen"
  ],
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@octokit/webhooks": "7.15.1",
    "@tomsun28/google-translate-api": "1.1.2",
    "franc-min": "5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/langs": "^2.0.4",
    "@types/node": "^14.10.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vercel/ncc": "^0.38.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-cmyr": "^1.1.30",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^23.20.0",
    "jest": "^24.9.0",
    "jest-circus": "^26.4.2",
    "js-yaml": "^3.14.0",
    "prettier": "2.1.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^24.3.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "overrides": {
    "@octokit/plugin-rest-endpoint-methods": "4.2.1"
  }
}
